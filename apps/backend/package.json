{
  "name": "backend",
  "version": "0.0.0",
  "description": "",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc --project tsconfig.json",
    "start": "node dist/index.js",
    "clean": "rimraf dist",
    "check-types": "tsc --noEmit",
    "nuke": "rimraf dist node_modules .turbo tsconfig.tsbuildinfo",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "db:schema:create": "NODE_OPTIONS='--import=tsx' npx mikro-orm schema:create",
    "db:schema:update": "NODE_OPTIONS='--import=tsx' npx mikro-orm schema:update --safe",
    "db:schema:drop": "NODE_OPTIONS='--import=tsx' npx mikro-orm schema:drop",
    "db:schema:fresh": "NODE_OPTIONS='--import=tsx' npx mikro-orm schema:fresh",
    "db:migration:create": "NODE_OPTIONS='--import=tsx' npx mikro-orm migration:create",
    "db:migration:up": "NODE_OPTIONS='--import=tsx' npx mikro-orm migration:up",
    "db:migration:down": "NODE_OPTIONS='--import=tsx' npx mikro-orm migration:down",
    "db:migration:list": "NODE_OPTIONS='--import=tsx' npx mikro-orm migration:list",
    "db:migration:pending": "NODE_OPTIONS='--import=tsx' npx mikro-orm migration:pending",
    "db:migration:fresh": "NODE_OPTIONS='--import=tsx' npx mikro-orm migration:fresh"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@1inch/limit-order-sdk": "5.0.3",
    "@ai-sdk/anthropic": "1.2.12",
    "@ai-sdk/deepseek": "0.2.16",
    "@ai-sdk/google": "1.2.22",
    "@ai-sdk/openai": "1.3.24",
    "@chatmcp/server-perplexity-ask": "^0.1.0",
    "@hatchet-dev/typescript-sdk": "^1.9.3",
    "@langchain/core": "^0.3.57",
    "@mikro-orm/cli": "^6.4.15",
    "@mikro-orm/core": "^6.4.15",
    "@mikro-orm/migrations": "^6.4.15",
    "@mikro-orm/postgresql": "^6.4.15",
    "@mikro-orm/reflection": "^6.4.15",
    "@modelcontextprotocol/server-brave-search": "^0.6.2",
    "@modelcontextprotocol/server-everart": "^0.6.2",
    "@openai/agents": "^0.0.16",
    "@openai/agents-extensions": "^0.0.16",
    "@openrouter/ai-sdk-provider": "0.7.5",
    "axios": "^1.9.0",
    "common": "workspace:*",
    "dayjs": "^1.11.13",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "find-up": "^7.0.0",
    "firebase-admin": "^13.4.0",
    "flatted": "^3.3.3",
    "linkup-mcp-server": "^1.1.0",
    "lodash-es": "^4.17.21",
    "morgan": "^1.10.0",
    "pg": "^8.15.6",
    "posthog-node": "^4.17.2",
    "redis": "^5.5.6",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0",
    "viem": "^2.31.7",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/express": "^5.0.1",
    "@types/lodash": "^4.17.18",
    "@types/lodash-es": "^4.17.12",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.15.3",
    "@types/winston": "^2.4.4",
    "@xyflow/react": "^12.6.4",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.2",
    "rimraf": "^6.0.1",
    "tsx": "^4.7.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.26.1"
  },
  "mikro-orm": {
    "alwaysAllowTs": true,
    "useTsNode": false,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.9",
    "utf-8-validate": "^6.0.5"
  }
}
